        "from collections import defaultdict\n",
        "\n",
        "\n",
        "# Calculate word frequencies for each class on train data\n",
        "def calculate_word_frequencies(\n",
        "    X: List[List[str]], y: List[int], num_labels: int = 3\n",
        ") -> Tuple[Dict[str, List[int]], List[int]]:\n",
        "    \"\"\"\n",
        "    Calculate word frequencies for each class in the dataset.\n",
        "\n",
        "    Args:\n",
        "        X - List of tokenized sentences.\n",
        "        y - List of labels corresponding to the sentences.\n",
        "        num_labels - Number of unique labels in the dataset.\n",
        "\n",
        "    Returns:\n",
        "        word_counts - Dictionary mapping words to their frequency counts for each class.\n",
        "        class_counts - List of counts for each class.\"\"\"\n",
        "    # Your code here!\n",
        "    word_counts = defaultdict(lambda: [0] * num_labels)\n",
        "    class_word_counts = [0] * num_labels\n",
        "\n",
        "    for sentence_tokens, label_idx in zip(X, y):\n",
        "        class_word_counts[label_idx] += len(sentence_tokens)\n",
        "        for word in sentence_tokens:\n",
        "            word_counts[word][label_idx] += 1\n",
        "\n",
        "    return word_counts, class_word_counts\n",
        "\n",
        "\n",
        "def compute_pmi(\n",
        "    word_counts: Dict[str, List[int]], class_counts: List[int]\n",
        ") -> Dict[Tuple[str, int], float]:\n",
        "    \"\"\"\n",
        "    Compute Pointwise Mutual Information (PMI) for each word in each class.\n",
        "\n",
        "    Args:\n",
        "        word_counts - Dictionary mapping words to their frequency counts for each class.\n",
        "        class_counts - List of counts for each class.\n",
        "\n",
        "    Returns:\n",
        "        pmi - Dictionary mapping (word, class_index) to PMI score.\"\"\"\n",
        "    # Your code here!\n",
        "    pmi_scores = {}\n",
        "    num_labels = len(class_counts)\n",
        "    total_corpus_words = sum(class_counts)\n",
        "\n",
        "    if total_corpus_words == 0:\n",
        "        raise ValueError(\"Total corpus word count is zero, cannot compute PMI.\")\n",
        "\n",
        "    word_totals_in_corpus = {word: sum(counts) for word, counts in word_counts.items()}\n",
        "\n",
        "    # P_word\n",
        "    for word, counts in word_counts.items():\n",
        "        total_word_occurrences = word_totals_in_corpus[word]\n",
        "        if total_word_occurrences == 0:\n",
        "            continue\n",
        "        p_word = total_word_occurrences / total_corpus_words\n",
        "\n",
        "        # P_class\n",
        "        for class_index in range(num_labels):\n",
        "            class_count = class_counts[class_index]\n",
        "            if class_count == 0:\n",
        "                continue\n",
        "            p_class = class_count / total_corpus_words\n",
        "\n",
        "            # P_word, class\n",
        "            p_word_class = (\n",
        "                counts[class_index] / total_corpus_words\n",
        "                if total_corpus_words > 0\n",
        "                else 0\n",
        "            )\n",
        "\n",
        "            denominator = p_word * p_class\n",
        "\n",
        "            # PMI calculation\n",
        "            pmi_val = math.log(p_word_class / denominator, 2) if p_word_class > 0 else 0\n",
        "            pmi_scores[(word, class_index)] = pmi_val\n",
        "\n",
        "    return pmi_scores\n",
        "\n",
        "\n",
        "# Print top features (words) for each class based on PMI\n",
