        "from sklearn.tree import DecisionTreeClassifier\n",
        "\n",
        "def top_n_features_overall(pmi: Dict[Tuple[str, int], float], N: int) -> List[str]:\n",
        "    \"\"\"Get top N features based on overall PMI scores.\n",
        "\n",
        "    Args:\n",
        "        pmi - Dictionary mapping (word, class_index) to PMI scores.\n",
        "        N - Number of top features to return.\n",
        "    Returns:\n",
        "        top_features - List of top N features based on overall PMI scores.\n",
        "    \"\"\"\n",
        "    # Your code here!\n",
        "    max_pmi_per_word = defaultdict(lambda: -float(\"inf\"))\n",
        "    for (word, class_index), pmi_score in pmi.items():\n",
        "        if pmi_score > max_pmi_per_word[word]:\n",
        "            max_pmi_per_word[word] = pmi_score\n",
        "\n",
        "    # Sort words by their maximum PMI score and take the top N\n",
        "    sorted_features = sorted(\n",
        "        max_pmi_per_word.items(), key=lambda x: x[1], reverse=True\n",
        "    )[:N]\n",
        "\n",
        "    top_features = [word for word, _ in sorted_features[:N]]\n",
        "\n",
        "    return top_features\n",
        "\n",
        "\n",
        "# Determine number of unique words\n",
        "num_unique_words = len(word_freq_counts)\n",
        "\n",
        "\n",
        "# Define intervals for N\n",
        "N_values = np.linspace(10, num_unique_words, 20, dtype=int)\n",
        "accuracies_dt = []\n",
        "\n",
        "# Train decision tree classifier for each value of N\n",
        "for N in N_values:\n",
        "    top_N_vocab = top_n_features_overall(pmi, N)\n",
        "\n",
        "    vectorizer_pmi = CountVectorizer(vocabulary=top_N_vocab)\n",
        "    X_train_pmi = vectorizer_pmi.fit_transform(X_train_tweet)\n",
